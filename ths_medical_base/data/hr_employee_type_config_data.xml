<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="emp_type_config_employee" model="ths.hr.employee.type.config">
            <field name="name">Employee</field>
            <field name="employee_type_key">employee</field>
            <field name="is_medical">False</field>
        </record>

        <record id="emp_type_config_contractor" model="ths.hr.employee.type.config">
            <field name="name">Contractor</field>
