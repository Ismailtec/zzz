<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_stock_quant_tree_inventory_editable_inherit_ths_base" model="ir.ui.view">
            <field name="name">stock.quant.tree.inventory.editable.inherit.ths.base</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree_inventory_editable"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='inventory_quantity']" position="after">
                    <field name="ths_effective_date" optional="show"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>